<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
		<property name="hibernate.current_session_context_class">thread</property>
		<property name="show_sql">true</property>
		<property name="format_sql">true</property>
		
		<mapping class="com.fund.model.Announcement"/>
		<mapping class="com.fund.model.Assess"/>
		<mapping class="com.fund.model.FundMsg"/>
		<mapping class="com.fund.model.FundOrder"/>
		<mapping class="com.fund.model.FundProduct"/>
		<mapping class="com.fund.model.FundProject"/>
		<mapping class="com.fund.model.FundReply"/>
		<mapping class="com.fund.model.FundReturn"/>
		<mapping class="com.fund.model.FundSettle"/>
		<mapping class="com.fund.model.FundStock"/>
		<mapping class="com.fund.model.Manager"/>
		<mapping class="com.fund.model.Member"/>
		<mapping class="com.fund.model.Msg"/>
		<mapping class="com.fund.model.Order"/>
		<mapping class="com.fund.model.OrderDetail"/>
		<mapping class="com.fund.model.Product"/>
		<mapping class="com.fund.model.Reply"/>
		<mapping class="com.fund.model.Report"/>
		<mapping class="com.fund.model.Return"/>
		<mapping class="com.fund.model.Stock"/>
		<mapping class="com.fund.model.Store"/>

	</session-factory>


</hibernate-configuration>
